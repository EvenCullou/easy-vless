{
  "name": "easy-vle",
  "description": "部署在Heroku上的工具",
  "keywords": [
    "2ray VMe VLE"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议VMess或VLESS，请填小写的vmess或vless",
      "value": "vmess",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/EvenCullou/easy-vless",
  "repository": "https://github.com/EvenCullou/easy-vless",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
